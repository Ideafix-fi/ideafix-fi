<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="timesheet_view_tree_user_inherit_inherit_ideafix_poc" model="ir.ui.view">
        <field name="name">timesheet.view.tree.user.inherit.inherit.ideafix.project.and.field.customization</field>
        <field name="model">account.analytic.line</field>
        <field name="inherit_id" ref="industry_fsm.timesheet_view_tree_user_inherit"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="start_longitude"/>
                <field name="start_latitude"/>
                <field name="end_longitude"/>
                <field name="end_latitude"/>
            </xpath>
        </field>
    </record>
</odoo>